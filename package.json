{
  "name": "codewars-tasks",
  "version": "1.0.0",
  "repository": "git@github.com:AlekseyLeshko/codewars-tasks.git",
  "author": "AlekseyLeshko <alekseyleshko@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "ava **/test.js",
    "test:watch": "ava **/test.js --watch",
    "precommit": "lint-staged"
  },
  "dependencies": {},
  "devDependencies": {
    "ava": "^0.25.0",
    "ava-spec": "^1.1.1",
    "babel-eslint": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-plugin-ava": "^4.5.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "import-sort-parser-babylon": "^4.2.0",
    "import-sort-style-eslint": "^4.2.0",
    "lint-staged": "^7.0.0",
    "prettier": "^1.11.1"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "eslint"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add"
    ]
  }
}
